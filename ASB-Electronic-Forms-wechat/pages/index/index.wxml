<view class="page-body">
  <view class="page-section page-section-spacing swiper">
    <swiper style="height:400rpx" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" circular="{{circular}}" interval="{{interval}}" duration="{{duration}}">
      <block wx:for="{{imgUrls}}" wx:key="*this">
        <swiper-item>
          <image class='center-swiper' src="{{item}}" class="swiper-item" width="200rpx" height="400rpx"/>
        </swiper-item>
      </block>
    </swiper>
  </view>
</view>

<view class="userinfo">
  <text style='margin-top: 10rpx; font-size: 40rpx;'>Welcome!</text>
  <open-data type="userNickName" style='font-size: 40rpx'></open-data>
</view>

<button
disabled="{{adminButtonDisabled}}"
hidden='{{adminButtonDisabled}}'
class='goButton'
bindtap='onAdminButton'>
  Check Reservations (ASB)
</button>

<view class="row-container">
  <text class='smallText'>_________________</text>
  <text class='smallText'>1. Correctly fill in your name, grade, and class</text>
  <text class='smallText'>2. Reserve your treats for December Delights</text>
  <text class='smallText'>3. Come to ZTB lobby during lunch to confirm</text>
  <text class='smallText'>_________________</text>
</view>